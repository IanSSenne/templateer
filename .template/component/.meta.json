{
  "insertAt": "dsrc/components/$Name",
  "files": ["$Name.tsx", "$Name.module.scss"],
  "postCreationActions": [
    "node \"$Root/post-create.js\" \"$Target\" \"$Name\""
  ],
  "prompts": [
    {
      "type": "text",
      "name": "Name",
      "message": "What is the name of the component?"
    }
  ],
  "defaultPlaceholders": {}
}
